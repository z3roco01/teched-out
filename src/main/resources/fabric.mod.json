{
	"schemaVersion": 1,
	"id": "teched_out",
	"version": "${version}",
	"name": "Teched Out",
	"description": "1.20.1 all encompassing tech mod !",
	"authors": [
		"z3roco01"
	],
	"contact": {
		"sources": "https://github.com/z3roco01/teched-out"
	},
	"license": "GPL-3.0",
	"icon": "assets/teched_out/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "z3roco01.techedout.TechedOut",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "z3roco01.techedout.TechedOutClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "z3roco01.techedout.data.TechedOutDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"teched-out.mixins.json",
		{
			"config": "teched-out.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}